    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "clean-webpack-plugin": "^0.1.8",
    "css-loader": "~0.23.1",
    "ejs": "^2.3.1",
    "empty-module": "0.0.2",
    "eslint": "<2.3.0",
    "eslint-loader": "^1.3.0",
    "express": "^4.13.4",
    "express-http-proxy": "^0.3.1",
    "extract-text-webpack-plugin": "^0.3.8",
