{
  "name": "webpack-react-boilerplate",
  "version": "1.0.0",
  "description": "A simple webpack boilerplate with hot loading and ES6",
  "git": "https://theneilmiester@bitbucket.org/theneilmiester/webpack-react-boilerplate.git",
  "main": "src/app.js",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --host 0.0.0.0 --port 3000",
    "build": "NODE_ENV=production webpack",
    "profile": "webpack --profile --json > stats.json",
    "dist": "gulp dist"
  },
